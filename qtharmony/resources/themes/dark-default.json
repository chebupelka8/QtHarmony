{
    "Info": {
        "name": "Dark-Default",
        "author": null
    },

    "MainWindow": {
        "object-name": "QMainWindow#main-window",
        
        ":active": {
            "background-color": "#252525",
            "border": "none",
            "border-radius": "none"
        }
    },

    "PushButton": {
        "object-name": "QPushButton#button",

        ":active": {
            "background-color": "#405cf5",
            "border": "none",
            "border-radius": "6px"
        },

        ":hover": {
            "background-color": "#3045B8"
        },

        ":pressed": {
            "background-color": "#283A9A"
        }
    },

    "DropDownMenu": {
        "object-name": "QComboBox#drop-down-menu",

        ":active": {
            "background-color": "#ffffff",
            "color": "#1A3B5D",
            "border": "1px solid #adb5bd",
            "border-radius": "6px"
        },

        ":hover": {
            "background-color": "6185DF"
        },

        " QAbstractItemView": {
            "background-color": "#ffffff"
        },

        "::drop-down": {
            "border": "none"
        }
    }
}